
{
  "alias": "php-cms-version-checker.phar",
  "compactors": ["Herrera\\Box\\Compactor\\Php"],
  "compression": "GZ",
  "directories": ["src"],
  "files": ["LICENSE", "vendor/herrera-io/phar-update/res/schema.json"],
  "finder": [
    {
      "name": "*.php",
      "exclude": ["test", "tests", "Tests"],
      "in": "vendor"
    }
  ],
  "git-version": "package_version",
  "main": "bin/php-cms-version-checker",
  "output": "php-cms-version-checker.phar",
  "stub": true
}